{
  "name": "sign",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "16",
    "@types/react-dom": "16",
    "node-sass-chokidar": "1",
    "npm-run-all": "4",
    "react": "16",
    "react-dom": "16",
    "react-scripts": "3",
    "reconnecting-websocket": "4",
    "typescript": "3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-ts",
    "start-ts": "react-scripts start",
    "build": "npm-run-all -p build-css build-ts",
    "build-ts": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
